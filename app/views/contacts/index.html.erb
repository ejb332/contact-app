<h1>Contacts</h1>
<div class="main">
<% @contacts.each do |contact| %>
  <h2><%= contact.first_name %></h2>
  <p>Full name: <%= contact.full_name %></p>
  <p>Age: <%= contact.age %></p>
  <p>Gender: <%= contact.gender %></p>
  <p>Exact geographical coordinates of diary: <%= contact.diary %></p>
  <p>Added: <%= contact.friendly_created_at %></p>
</div>
<% end %>
<br>
<div class="btn btn-primary">
  <a href="/contacts/new">Add new contact</a>
</div>